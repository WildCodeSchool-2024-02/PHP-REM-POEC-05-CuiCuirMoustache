{% extends 'layout.html.twig' %}

{% block stylesheet %}
<link rel="stylesheet" href="/assets/css/product/product.css">
{% endblock %}

{% block content %}
<div class="parent-div">

    <div class="div1">
        <div class="product-image"><img src="/assets/images/uploads/{{ product.image }}" alt="image-produit" class="prod-img"></div>
        <div class="div3" id="div3">
            <p class="long-description">{{ product.descriptionDetail }}</p>
        </div>
        <div class="review div4" id="div4">Avis clients
            <p>Aucun avis pour le moment</p>
        </div>
    </div>
    <div class="div2">
        <h2><b>{{ product.name }}</b></h2>
        <p class="description">{{ product.description }}</p>
        <p class="date">Produit disponible depuis le {{ product.created_at| date("m/d/Y") }}</p>
        <h3><b>{{ product.price|intToCurrency }}</b></h3>
        <form method="POST" action="/cart/add" class="form-css noselect">
            <input type="hidden" name="id" value="{{ product.id }}">
            <input type="number" name="qty" value="1" class="inp-numb">
            <button class="add">Ajouter au panier</button>
        </form>
        <div class="conditions">
            <p>99% des articles en stock</p>
            <p>Expédition 24/48h (jours ouvrés)</p>
            <p>Paiement sécurisé et 3x sans frais</p>
        </div>
    </div>
    <div class="div5" id="div5"></div>
</div>

